#[derive(Debug)]
enum Shirts {
    Red,
    Green,
    Blue,
}

#[derive(Debug)]
struct Store {
    shirts: vec<Shirts>,
}

impl Store {
    fn get_all(&self) {
        for colors in &self.shirts {
            println!("{:?}", colors);
        }
    }
}

fn main() {
    let iventory: Vec<Shirts> = vec![Shirts::Red, Shirts::Green, Shirts::Blue, Shirts::Blue];
    let choice = Some(Shirts::Red);
    for colors in &iventory {
        println!("{:?}", colors);
    }
}
